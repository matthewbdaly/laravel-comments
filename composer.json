{
    "name": "matthewbdaly/laravel-comments",
    "description": "Simple Laravel comment system",
    "keywords": ["laravel","comments"],
    "type": "library",
    "require-dev": {
        "orchestra/testbench": "^3.5",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.4",
        "squizlabs/php_codesniffer": "^3.1",
        "satooshi/php-coveralls": "^1.0",
        "psy/psysh": "^0.8.14"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Matthew Daly",
            "email": "matthewbdaly@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Matthewbdaly\\LaravelComments\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Matthewbdaly\\LaravelComments\\Providers\\CommenttServiceProvider"
            ]
        }
    }
}
